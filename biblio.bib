isbn = {0733-9429/2003/8-585-596},
issn = {0733-9429},
journal = {Journal of Hydraulic Engineering},
number = {8},
pages = {585--596},
title = {{Influence of Turbulence on Bed Load Sediment Transport}},
url = {http://ascelibrary.org/doi/10.1061/{\%}28ASCE{\%}290733-9429{\%}282003{\%}29129{\%}3A8{\%}28585{\%}29},
volume = {129},
