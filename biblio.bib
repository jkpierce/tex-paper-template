volume = {92},
year = {2008}
}
@article{Adler2018,
abstract = {An improved synthetic digital filtering method (SDFM) for the generation of equilibrium turbulence is described, with emphasis on the ease of application to complex configurations. This is achieved by facilitating the use of modeled (RANS) statistics for the mean flow and Reynolds stress profiles, instead of simulated (DNS or LES) statistics that are not readily available for three-dimensional flows. The effects on boundary layer relaxation with modeled statistics are first quantified for a well-known canonical situation, specifically, Mach 2 equilibrium turbulent boundary layers over 250 {\textless} Re$\theta${\textless} 2220. Various convergence criteria are assessed in terms of the streamwise coordinate (x) normalized by the incoming boundary layer thickness ($\delta$0). These include skin-friction, which requires x/$\delta$0≈ 12 for equilibrium, inner scale mean velocity (x/$\delta$0≈ 16), inner scale Reynolds stresses (x/$\delta$0≈ 18), as well as outer scale mean velocity, Reynolds stresses, and spatio-temporal correlations (x/$\delta$0≈ 40–50). Development lengths are shown to be inversely related to Reynolds number. In terms of boundary layer relaxation length, initializing the method with RANS (versus simulated) statistics incurs a negligible penalty in inner coordinates and a mild penalty (x/$\delta$0≈ 5–10) in outer coordinates. The effectiveness of this approach is then demonstrated by application to significantly more complex three-dimensional flows, including internal single and multi-stream flows, for which synthetic turbulence generation techniques are necessary, but precursor high-fidelity simulations are not available. Thus, the RANS-initialized SDFM provides a rapidly adaptable method for the synthetic generation of wall-bounded turbulence.},
author = {Adler, Michael C. and Gonzalez, David R. and Stack, Cory M. and Gaitonde, Datta V.},
doi = {10.1016/j.compfluid.2018.01.003},
file = {:home/kpierce/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Adler et al. - 2018 - Synthetic generation of equilibrium boundary layer turbulence from modeled statistics.pdf:pdf},
issn = {00457930},
journal = {Computers and Fluids},
keywords = {Compressible flow,Large-eddy simulations,Multi-stream internal flows,Synthetic turbulence generation,Turbulent boundary layers},
pages = {127--143},
publisher = {Elsevier Ltd},
title = {{Synthetic generation of equilibrium boundary layer turbulence from modeled statistics}},
url = {https://doi.org/10.1016/j.compfluid.2018.01.003},
volume = {165},
year = {2018}
}
