file = {:home/kpierce/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Moir, Pasternack - 2008 - Relationships between mesoscale morphological units, stream hydraulics and Chinook salmon (Oncorhynchus tshawy.pdf:pdf},
isbn = {0169-555X},
issn = {0169555X},
journal = {Geomorphology},
keywords = {Chinook salmon,Fluvial geomorphology,Hydraulics,Morphological units,Spawning,Two-dimensional modeling},
number = {3-4},
pages = {527--548},
title = {{Relationships between mesoscale morphological units, stream hydraulics and Chinook salmon (Oncorhynchus tshawytscha) spawning habitat on the Lower Yuba River, California}},
