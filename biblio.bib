journal = {Sedimentology of Gravels and Conglomerates},
keywords = {1984,77- as,and steel,canadian society of petroleum,characteristics and origin of,cluster bedforms,comglomerates,e,eds,geologists,h,in coarse-grained alluvial channels,j,koster,memoir 10,ogy of gravels and,p,r,sedimento},
