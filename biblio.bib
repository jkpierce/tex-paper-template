@article{Stock2003,
abstract = {The sculpture of valleys by flowing water is widely recognized, and$\backslash$nsimplified models of incision by this process (e.g., the stream power$\backslash$nlaw) are the basis for most recent landscape evolution models. Under$\backslash$nsteady state conditions a stream power law predicts that channel$\backslash$nslope varies as an inverse power law of drainage area. Using both$\backslash$ncontour maps and laser altimetry, we find that this inverse power$\backslash$nlaw rarely extends to slopes greater than $\backslash${\~{}}0.03 to 0.10, values$\backslash$nbelow which debris flows rarely travel. Instead, with decreasing$\backslash$ndrainage area the rate of increase in slope declines, leading to$\backslash$na curved relationship on a log-log plot of slope against drainage$\backslash$narea. Fieldwork in the western United States and Taiwan indicates$\backslash$nthat debris flow incision of bedrock valley floors tends to terminate$\backslash$nupstream of where strath terraces begin and where area-slope data$\backslash$nfollow fluvial power laws. These observations lead us to propose$\backslash$nthat the steeper portions of unglaciated valley networks of landscapes$\backslash$nsteep enough to produce mass failures are predominately cut by debris$\backslash$nflows, whose topographic signature is an area-slope plot that curves$\backslash$nin log-log space. This matters greatly as valleys with curved area-slope$\backslash$nplots are both extensive by length ({\textgreater}80{\%} of large steepland basins)$\backslash$nand comprise large fractions of main stem valley relief (25�100{\%}).$\backslash$nAs a consequence, valleys carved by debris flows, not rivers, bound$\backslash$nmost hillslopes in unglaciated steeplands. Debris flow scour of these$\backslash$nvalleys appears to limit the height of some mountains to substantially$\backslash$nlower elevations than river incision laws would predict, an effect$\backslash$nabsent in current landscape evolution models. We anticipate that$\backslash$nan understanding of debris flow incision, for which we currently$\backslash$nlack even an empirical expression, would substantially change model$\backslash$nresults and inferences drawn about linkages between landscape morphology$\backslash$nand tectonics, climate, and geology.},
author = {Stock, J. and Dietrich, W. E.},
doi = {10.1029/2001WR001057},
file = {:home/kpierce/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Stock, Dietrich - 2003 - Valley incision by debris flows Evidence of a topographic signature.pdf:pdf},
isbn = {0043-1397},
issn = {00431397},
journal = {Water Resources Research},
keywords = {http://dx.doi.org/10.1029/2001WR001057, doi:10.102},
number = {4},
title = {{Valley incision by debris flows: Evidence of a topographic signature}},
url = {http://doi.wiley.com/10.1029/2001WR001057},
volume = {39},
year = {2003}
}
@article{Meade1985,
abstract = {Bedload is moved down the East Fork River in distinct wavelike pulses that have the form of composite dune fields The moving material consists mostly of coarse sand and fine gravel The wavelengths of the pulses are about 500–600 m, a distance that is predetermined by the pattern of storage of bed sediment in the river during low water As the river discharge increases, the bed sediment is scoured from the storage areas, and it is moved onto and across the interventing riffles As the river discharge decreases, the bed sediment is scoured off the riffles and moved into the next storage area downstream Each successive pulse of water discharge sets into motion a wave of bedload that continues to move unitil it reaches the next storage area},
