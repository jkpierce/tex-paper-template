@article{Radice2009,
abstract = {Data from a long-duration laboratory experiment involving optical$\backslash$nmeasurements of moving particles in a bed load layer are presented.$\backslash$nThe data analysis is focused on identification of (1) the effects$\backslash$nof the spatial measuring scale on concentration estimates, (2) statistical$\backslash$nand scaling properties of sediment concentration, and (3) relationships$\backslash$nbetween near-bed turbulent flow and sediment motion. The obtained$\backslash$nresults are integrated into a conceptual model that can be instrumental$\backslash$nin studying statistical properties of solid transport rate, which,$\backslash$nin most cases, can be linearly associated with the particle concentration.},
author = {Radice, Alessio and Ballio, Francesco and Nikora, Vladimir},
