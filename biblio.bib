doi = {10.1007/s00339-008-4547-6},
file = {:home/kpierce/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Shi, Zhang - 2008 - Simulation of random packing of spherical particles with different size distributions.pdf:pdf},
isbn = {0947-8396},
