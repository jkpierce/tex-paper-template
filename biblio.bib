doi = {10.1086/376764},
file = {:home/kpierce/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Malmon et al. - 2003 - Stochastic Theory of Particle Trajectories through Alluvial Valley Floors.pdf:pdf},
isbn = {00221376},
